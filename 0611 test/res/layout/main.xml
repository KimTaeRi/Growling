<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/growling_main" >

    <Button
        android:id="@+id/Delivery"
        android:layout_width="210dip"
        android:layout_height="64dip"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="260dip"
        android:background="@drawable/delivery" />

    <Button
        android:id="@+id/NoneDelivery"
        android:layout_width="210dip"
        android:layout_height="64dip"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/Delivery"
        android:layout_marginTop="25dip"
        android:background="@drawable/none_delivery" />

    <Button
        android:id="@+id/ShcoolRestraunt"
        android:layout_width="210dip"
        android:layout_height="64dip"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dip"
        android:layout_below="@+id/NoneDelivery"
        android:background="@drawable/school" />

</RelativeLayout>